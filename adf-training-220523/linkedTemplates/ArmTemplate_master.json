{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-training-220523"
		},
		"LS_SQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=myadftraining220523.database.windows.net;Initial Catalog=dev;User ID=saadmin"
		},
		"LS_ADLS_GENERIC_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().ADLS_URL}"
		},
		"LS_ADSL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsadftraining2023.dfs.core.windows.net/"
		},
		"LS_KEYVAULT_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvadftrainnig.vault.azure.net/"
		},
		"LS_REST_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://restapi.adequateshop.com"
		},
		"LS_SQL_GENERIC_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "@linkedService().ConnectionSecret"
		},
		"storageEventTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/9525a20f-efc2-4a27-ab1d-b37252baa537/resourceGroups/adftraining2023/providers/Microsoft.Storage/storageAccounts/adlsadftraining2023"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-training-220523_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_SQL_connectionString": {
						"value": "[parameters('LS_SQL_connectionString')]"
					},
					"LS_ADSL_properties_typeProperties_url": {
						"value": "[parameters('LS_ADSL_properties_typeProperties_url')]"
					},
					"LS_KEYVAULT_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KEYVAULT_properties_typeProperties_baseUrl')]"
					},
					"LS_REST_properties_typeProperties_url": {
						"value": "[parameters('LS_REST_properties_typeProperties_url')]"
					},
					"LS_ADLS_GENERIC_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_GENERIC_properties_typeProperties_url')]"
					},
					"LS_SQL_GENERIC_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_SQL_GENERIC_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-training-220523_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"storageEventTrigger_properties_typeProperties_scope": {
						"value": "[parameters('storageEventTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-training-220523_ArmTemplate_0')]"
			]
		}
	]
}